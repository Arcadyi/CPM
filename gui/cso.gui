types cso_button_icons
{	
    type button_icon_up_action = button_icon_round_action
	{
		blockoverride "sound" {
			using = increase_button_sound
		}
		blockoverride "icon" {
			texture = "gfx/interface/cso/arrow_up.dds"
		}
	}

	type button_icon_down_action = button_icon_round_action
	{
		blockoverride "sound" {
			using = decrease_button_sound
		}
		blockoverride "icon" {
			texture = "gfx/interface/cso/arrow_down.dds"
		}
	}
}